{
  "name": "c12",
  "version": "0.2.7",
  "description": "Smart Config Loader",
  "repository": "unjs/c12",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "prepack": "unbuild",
    "release": "pnpm test && standard-version && git push --follow-tags && pnpm publish",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "defu": "^6.0.0",
    "dotenv": "^16.0.1",
    "gittar": "^0.1.1",
    "jiti": "^1.14.0",
    "mlly": "^0.5.4",
    "pathe": "^0.3.2",
    "rc9": "^1.2.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "c8": "latest",
    "eslint": "latest",
    "standard-version": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  },
  "packageManager": "pnpm@7.4.0"
}
